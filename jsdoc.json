{
	"opts": {
		"destination": "docs",
		"pedantic": true,
		"readme": "modules/README.md",
		"recurse": true,
		"template": "node_modules/jsdoc-wmf-theme"
	},
	"plugins": [
		"node_modules/jsdoc-wmf-theme/plugins/default"
	],
	"source": {
		"include": [ "modules" ],
		"exclude": [
			"modules/ext.eventLogging.debug/debug.js",
			"modules/lib/"
		]
	},
	"templates": {
		"cleverLinks": true,
		"default": {
			"useLongnameInNav": true
		},
		"wmf": {
			"maintitle": "EventLogging",
			"repository": "https://gerrit.wikimedia.org/g/mediawiki/extensions/EventLogging/",
			"siteMap": {
				"sections": true
			},
			"linkMap": {
				"jQuery.Promise": "https://api.jquery.com/Types/#Promise"
			}
		}
	}
}
